{
    "name": "@basic/remote-module",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Remote module to showcase Squide.",
    "private": true,
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "dev": "rsbuild dev --config rsbuild.dev.ts",
        "dev-isolated": "cross-env ISOLATED=true pnpm dev",
        "build": "rsbuild build --config rsbuild.build.ts",
        "serve-build": "pnpm http-server dist -p 8081 -P http://localhost:8081? -c-1",
        "deploy": "netlify deploy --dir=samples/basic/remote-module/dist --site=43234f6e-a884-410e-9b4d-f290459f841f --prod",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsgo"
    },
    "dependencies": {
        "@basic/shared": "workspace:*",
        "@basic/shell": "workspace:*",
        "@react-aria/toast": "3.0.10",
        "@react-stately/toast": "3.1.3",
        "@squide/fakes": "workspace:*",
        "@squide/firefly": "workspace:*",
        "@workleap/logging": "1.3.6",
        "react": "19.2.4",
        "react-dom": "19.2.4",
        "react-router": "7.13.0",
        "useless-lib": "^2.1.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rsbuild/core": "1.7.3",
        "@squide/firefly-rsbuild-configs": "workspace:*",
        "@types/react": "19.2.14",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.56.0",
        "@typescript/native-preview": "7.0.0-dev.20260222.1",
        "@workleap/browserslist-config": "2.1.7",
        "@workleap/eslint-configs": "1.1.13",
        "@workleap/typescript-configs": "3.0.7",
        "browserslist": "4.28.1",
        "cross-env": "10.1.0",
        "eslint": "9.39.2",
        "http-server": "14.1.1",
        "netlify-cli": "23.15.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.54.0"
    }
}
