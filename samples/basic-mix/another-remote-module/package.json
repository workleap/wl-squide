{
    "name": "@basic-mix/another-remote-module",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Another remote module to showcase Squide.",
    "private": true,
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "dev": "webpack serve --config webpack.dev.js",
        "dev-isolated": "cross-env ISOLATED=true pnpm dev",
        "build": "webpack --config webpack.build.js",
        "serve-build": "pnpm http-server dist -p 8085 -P http://localhost:8085? -c-1",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@basic-mix/shared": "workspace:*",
        "@basic-mix/shell": "workspace:*",
        "@react-aria/toast": "3.0.7",
        "@react-stately/toast": "3.1.2",
        "@squide/fakes": "workspace:*",
        "@squide/firefly": "workspace:*",
        "@workleap/logging": "1.3.0",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "react-router": "7.8.2",
        "useless-lib": "3.0.0"
    },
    "devDependencies": {
        "@squide/firefly-webpack-configs": "workspace:*",
        "@swc/core": "1.13.5",
        "@swc/helpers": "0.5.17",
        "@types/react": "19.1.12",
        "@types/react-dom": "19.1.9",
        "@typescript-eslint/parser": "8.42.0",
        "@workleap/browserslist-config": "2.1.0",
        "@workleap/eslint-plugin": "3.5.0",
        "@workleap/swc-configs": "2.3.0",
        "@workleap/typescript-configs": "3.0.4",
        "browserslist": "4.25.4",
        "cross-env": "10.0.0",
        "eslint": "8.57.0",
        "http-server": "14.1.1",
        "typescript": "5.9.2",
        "webpack": "5.101.3",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "5.2.2"
    },
    "engines": {
        "node": ">=21.1.0"
    }
}
