{
    "name": "@squide/firefly-rsbuild-storybook",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Squide firefly helpers for Storybook and Rsbuild.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/firefly-rsbuild-storybook"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "peerDependencies": {
        "@opentelemetry/api": "^1.9.0",
        "@rsbuild/core": "^1.6.9",
        "@tanstack/react-query": "^5.90.11",
        "msw": "^2.12.3",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0",
        "react-router": "^7.9.6",
        "storybook": "^10.1.0",
        "storybook-react-rsbuild": "^3.0.0"
    },
    "peerDependenciesMeta": {
        "@opentelemetry/api": {
            "optional": true
        }
    },
    "dependencies": {
        "@squide/env-vars": "workspace:*",
        "@squide/firefly": "workspace:*",
        "@squide/msw": "workspace:*",
        "@workleap-telemetry/core": "^1.0.4",
        "@workleap/logging": "^1.3.2"
    },
    "devDependencies": {
        "@eslint/js": "9.39.1",
        "@rsbuild/core": "1.6.9",
        "@rslib/core": "0.18.2",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.48.0",
        "@workleap/eslint-configs": "1.1.5",
        "@workleap/rslib-configs": "1.1.3",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.48.0"
    },
    "sideEffects": false
}
