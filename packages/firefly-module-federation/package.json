        "react-dom": "^18.0.0 || ^19.0.0",
        "react-router": "^7.9.4"
    },
    "dependencies": {
        "@module-federation/enhanced": "0.11.4",
        "@rspack/core": "^1.5.8",
        "@squide/core": "workspace:*",
        "@squide/firefly": "workspace:*",
        "@workleap/logging": "^1.3.1"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@rsbuild/core": "1.5.17",
        "@rslib/core": "0.16.1",
