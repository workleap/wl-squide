{
    "name": "@squide/firefly",
    "author": "Workleap",
    "version": "16.0.2",
    "description": "Squide bundle for the firefly technology stack.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/firefly"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            },
            "./internal": {
                "types": "./dist/internal.d.ts",
                "import": "./dist/internal.js",
                "default": "./dist/internal.js"
            }
        }
    },
    "exports": {
        ".": "./src/index.ts",
        "./internal": "./src/internal.ts",
        "./tests": "./tests/utils.ts"
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "@opentelemetry/api": "^1.9.0",
        "@tanstack/react-query": "^5.90.10",
        "msw": "^2.12.2",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0",
        "react-router": "^7.9.6"
    },
    "peerDependenciesMeta": {
        "@opentelemetry/api": {
            "optional": true
        }
    },
    "dependencies": {
        "@squide/core": "workspace:*",
        "@squide/env-vars": "workspace:*",
        "@squide/msw": "workspace:*",
        "@squide/react-router": "workspace:*",
        "@workleap-telemetry/core": "^1.0.4",
        "@workleap/logging": "^1.3.2",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.1",
        "@rsbuild/core": "1.6.7",
        "@rslib/core": "0.18.0",
        "@testing-library/react": "16.3.0",
        "@types/react": "19.2.6",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.47.0",
        "@vitejs/plugin-react": "5.1.1",
        "@workleap/eslint-configs": "1.1.5",
        "@workleap/rslib-configs": "1.1.3",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.1",
        "happy-dom": "20.0.10",
        "typescript": "5.9.3",
        "typescript-eslint": "8.47.0",
        "vitest": "4.0.10"
    },
    "sideEffects": false
}
