{
    "name": "@squide/i18next",
    "author": "Workleap",
    "version": "2.2.8",
    "description": "Add support for i18next to @squide application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/i18next"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "i18next": "^25.2.1",
        "i18next-browser-languagedetector": "^8.1.0",
        "react": "^18.0.0 || ^19.0.0",
        "react-i18next": "^15.5.2"
    },
    "dependencies": {
        "@formatjs/intl-localematcher": "^0.6.1",
        "@squide/core": "workspace:*"
    },
    "devDependencies": {
        "@rsbuild/core": "1.3.21",
        "@rslib/core": "0.8.0",
        "@types/react": "19.1.5",
        "@typescript-eslint/parser": "8.32.1",
        "@workleap/eslint-plugin": "3.4.2",
        "@workleap/rslib-configs": "1.0.6",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.0",
        "typescript": "5.8.3",
        "vitest": "3.1.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
