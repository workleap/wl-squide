        "@types/react": "19.2.2",
        "@types/react-dom": "19.2.2",
        "@typescript-eslint/parser": "8.46.2",
        "@vitejs/plugin-react": "5.1.0",
        "@workleap/eslint-configs": "1.0.1",
        "@workleap/rslib-configs": "1.1.3",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.38.0",
        "happy-dom": "20.0.8",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2",
        "vitest": "4.0.4"
