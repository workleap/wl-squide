{
    "name": "@storybook/host",
    "description": "ShareGate Home application",
    "private": true,
    "author": "Workleap",
    "version": "0.0.0",
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "postinstall": "pnpm msw init",
        "dev": "rsbuild dev --config rsbuild.dev.ts",
        "build": "rsbuild build --config rsbuild.build.ts",
        "serve-build": "pnpm http-server dist/app -p 8081 -P http://localhost:8081? -c-1 --cors",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsgo"
    },
    "dependencies": {
        "@opentelemetry/api": "1.9.0",
        "@squide/firefly": "workspace:*",
        "@tanstack/react-query": "5.90.21",
        "launchdarkly-js-client-sdk": "3.9.0",
        "react": "19.2.4",
        "react-dom": "19.2.4",
        "react-router": "7.13.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rsbuild/core": "1.7.3",
        "@rslib/core": "0.19.6",
        "@rspack/core": "1.7.6",
        "@squide/firefly-rsbuild-storybook": "workspace:*",
        "@types/react": "19.2.14",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.56.0",
        "@typescript/native-preview": "7.0.0-dev.20260222.1",
        "@workleap/browserslist-config": "2.1.7",
        "@workleap/eslint-configs": "1.1.13",
        "@workleap/rsbuild-configs": "3.2.2",
        "@workleap/stylelint-configs": "2.1.8",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.2",
        "http-server": "14.1.1",
        "msw": "2.12.10",
        "storybook": "10.2.10",
        "storybook-react-rsbuild": "3.2.3",
        "stylelint": "17.3.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.54.0"
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
