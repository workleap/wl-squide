{
    "name": "@squide/module-federation",
    "author": "Workleap",
    "version": "7.0.1",
    "description": "Add support for Module Federation to @squide application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/module-federation"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "dependencies": {
        "@module-federation/enhanced": "0.11.4",
        "@rspack/core": "^1.5.8",
        "@squide/core": "workspace:*",
        "@workleap/logging": "^1.3.0"
    },
    "devDependencies": {
        "@rsbuild/core": "1.5.13",
        "@rslib/core": "0.15.0",
        "@typescript-eslint/parser": "8.46.0",
        "@workleap/eslint-plugin": "3.5.0",
        "@workleap/rslib-configs": "1.1.0",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.0",
        "typescript": "5.9.3",
        "vitest": "3.2.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
