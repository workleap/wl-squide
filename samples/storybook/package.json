{
    "name": "@storybook/app",
    "description": "Sample to showcase how Squide integrates with Storybook",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "postinstall": "pnpm msw init",
        "dev": "storybook dev -p 6006 --disable-telemetry",
        "build": "storybook build --disable-telemetry",
        "serve-build": "pnpm http-server storybook-static -p 8080 -P http://localhost:8080? -c-1",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsgo",
        "doctor": "storybook doctor"
    },
    "dependencies": {
        "@storybook/addon-a11y": "10.2.0",
        "msw": "2.12.7",
        "msw-storybook-addon": "2.0.6",
        "react": "19.2.4",
        "react-dom": "19.2.4",
        "react-router": "7.13.0",
        "storybook": "10.2.0",
        "storybook-react-rsbuild": "3.2.2"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rsbuild/core": "1.7.2",
        "@types/react": "19.2.9",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.54.0",
        "@typescript/native-preview": "7.0.0-dev.20260127.1",
        "@workleap/eslint-configs": "1.1.10",
        "@workleap/rsbuild-configs": "3.2.0",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.2",
        "http-server": "14.1.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.54.0"
    },
    "msw": {
        "workerDirectory": [
            ".storybook/public"
        ]
    }
}
