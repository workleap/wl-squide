{
    "name": "@squide/fakes",
    "author": "Workleap",
    "version": "2.1.11",
    "description": "Fake implementations to facilitate the development of modules in isolation with @squide.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/fakes"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@squide/core": "workspace:*"
    },
    "devDependencies": {
        "@rsbuild/core": "1.4.0",
        "@rslib/core": "0.10.3",
        "@typescript-eslint/parser": "8.35.0",
        "@workleap/eslint-plugin": "3.4.2",
        "@workleap/rslib-configs": "1.0.6",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.0",
        "typescript": "5.8.3"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
