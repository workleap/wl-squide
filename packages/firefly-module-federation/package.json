{
    "name": "@squide/firefly-module-federation",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Add support for Module Federation to @squide application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/firefly-module-federation"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "msw": "^2.11.6"
    },
    "dependencies": {
        "@module-federation/enhanced": "0.11.4",
        "@opentelemetry/api": "^1.9.0",
        "@rspack/core": "^1.5.8",
        "@squide/core": "workspace:*",
        "@squide/firefly": "workspace:*",
        "@workleap/logging": "^1.3.1"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@rsbuild/core": "1.5.17",
        "@rslib/core": "0.16.1",
        "@typescript-eslint/parser": "8.46.2",
        "@workleap/eslint-configs": "1.0.1",
        "@workleap/rslib-configs": "1.1.3",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.38.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2",
        "vitest": "4.0.4"
    },
    "sideEffects": false
}
