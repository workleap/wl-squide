{
    "name": "@basic-webpack/remote-module",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Remote module to showcase Squide.",
    "private": true,
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "dev": "webpack serve --config webpack.dev.js",
        "dev-isolated": "cross-env ISOLATED=true pnpm dev",
        "build": "webpack --config webpack.build.js",
        "serve-build": "pnpm http-server dist -p 8087 -P http://localhost:8087? -c-1",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@basic-webpack/shared": "workspace:*",
        "@basic-webpack/shell": "workspace:*",
        "@react-aria/toast": "3.0.8",
        "@react-stately/toast": "3.1.2",
        "@squide/fakes": "workspace:*",
        "@squide/firefly": "workspace:*",
        "@workleap/logging": "1.3.2",
        "react": "19.2.1",
        "react-dom": "19.2.1",
        "react-router": "7.10.1",
        "useless-lib": "^2.1.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.1",
        "@squide/firefly-webpack-configs": "workspace:*",
        "@swc/core": "1.15.3",
        "@swc/helpers": "0.5.17",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.49.0",
        "@workleap/browserslist-config": "2.1.3",
        "@workleap/eslint-configs": "1.1.6",
        "@workleap/swc-configs": "2.3.3",
        "@workleap/typescript-configs": "3.0.7",
        "browserslist": "4.28.1",
        "cross-env": "10.1.0",
        "eslint": "9.39.1",
        "http-server": "14.1.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.49.0",
        "webpack": "5.103.0",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "5.2.2"
    }
}
