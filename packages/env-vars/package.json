{
    "name": "@squide/env-vars",
    "author": "Workleap",
    "version": "1.4.15",
    "description": "Add support for environment variables to @squide application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/env-vars"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsgo",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0"
    },
    "dependencies": {
        "@squide/core": "workspace:*",
        "@workleap/logging": "^1.3.6",
        "memoize": "^10.2.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rsbuild/core": "1.7.3",
        "@rslib/core": "0.19.6",
        "@types/react": "19.2.14",
        "@typescript-eslint/parser": "8.56.0",
        "@typescript/native-preview": "7.0.0-dev.20260217.1",
        "@vitejs/plugin-react": "5.1.4",
        "@workleap/eslint-configs": "1.1.12",
        "@workleap/rslib-configs": "1.1.7",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.2",
        "happy-dom": "20.6.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.56.0",
        "vitest": "4.0.18"
    },
    "sideEffects": false
}
